  <div class='row'>
        <header class='row text-center bg-dark text-white'>
            <h1>Job Dashboard</h1>
        </header>
    </div>

    <main class='mt-10 mx-auto max-w-7xl px-4 sm:mt-12 sm:px-6 md:mt-16 lg:mt-20 lg:px-8 xl:mt-28'>
        <div class='column col-3 sm:text-center lg:text-left"'>
            <p class='search-header fs-3 block text-indigo-600 xl:inline'>Enter a position:</p>

            <input id='positionInput' type='text' name='position' class='form-control' placeholder='Enter Position' />
            <div id='display'></div>
            <span id='inputErrorPosition'></span>
        </div>

        <div class='column col-3 sm:text-center lg:text-left"'>
            <p class='search-header text-base text-indigo-600 tracking-wide'>Choose a location:</p>

            <input id='cityInput' type='text' name='city' class='form-control' placeholder='City Name' />
            <div id='display'></div>
            <span id='inputError'></span>
            <div id='recentHistory' class='recentHistory'>
                <button class="submitBtn mt-5 inline-flex items-center justify-center px-2.5 py-1.5 border border-transparent text-base font-medium rounded-md text-white bg-indigo-600 hover:bg-indigo-700" type='button' id='submitBtn'>Search Jobs</button>
            </div>
            <ul id='history-list' class='list-group'></ul>
        </div>

        <div id="job-results" class="container">
            <h2 id="five-title" class="mt-5 text-3xl leading-8 font-extrabold tracking-tight text-gray-900 sm:text-4xl">Top Results</h2>
            <div class="forecastRow row" id = "job-list"></div>
        </div>

<script src="./js/jobs.js"></script>